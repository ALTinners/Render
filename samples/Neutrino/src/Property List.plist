<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<array>
	<dict>
		<key>node:type</key>
		<string>UIKit.UIView</string>
		<key>node:reuseIdentifier</key>
		<string>wrapper-view</string>
		<key>view:backgroundColor</key>
		<string>#ccc</string>
		<key>view:yoga.padding</key>
		<string>float:50</string>
		<key>view:yoga.alignSelf</key>
		<string>yg-align:center</string>
		<key>view:yoga.maxWidth</key>
		<string>float:{{layout.size.width}}</string>
		<key>children</key>
		<array>
			<dict>
				<key>node:type</key>
				<string>UIKit.UILabel</string>
				<key>view:text</key>
				<string>{{props.text}}</string>
				<key>view:textColor</key>
				<string>color:{{#props.isImportant}}#ccc{{^props.isImportant}}#ddd{{/props.isImportant}}</string>
			</dict>
		</array>
	</dict>
</array>
</plist>
